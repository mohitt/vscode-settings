[
    {
        "key": "shift+alt+f",
        "command": "poor-mans-t-sql-formatter.formatSql",
        "when": "editorLangId == sql && editorTextFocus"
    },
    {
        "key": "escape",
        "command":"workbench.action.focusActiveEditorGroup",
        "when":"!editorTextFocus"
    }, 
]